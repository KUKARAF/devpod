#!/usr/bin/env /home/rafa/.config/dotfiles/.venv/bin/python
import sys
import requests

def main():
    text = sys.stdin.read()
    text = text.rstrip() + " " * 10
    url = "https://casa.osmosis.page/api/webhook/set_matrix_display"
    response = requests.post(url, data={"text": text})  # Remove headers, use data=

if __name__ == "__main__":
    main()
